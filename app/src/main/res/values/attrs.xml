<?xml version="1.0" encoding="UTF-8"?>
<resources>


    <!--侧滑删除-->
    <declare-styleable name="SwipeMenuLayout">
        <attr name="swipeEnable" format="boolean" />
        <attr name="ios" format="boolean" />
        <attr name="leftSwipe" format="boolean" />
    </declare-styleable>


    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_overlay" format="boolean" />
    </declare-styleable>
    <attr name="errorColor" format="color" />
    <attr name="maxCharacters" format="integer" />
    <declare-styleable name="materialEditText">
        <!-- Highlight color of floating label text and underline. -->
        <attr name="backgroundColor" format="color" />
        <!-- Error color of under line text and underline. -->
        <attr name="errorColor" />
        <!-- Enable floating label text. -->
        <attr name="floatingLabel" format="boolean" />
        <!-- Max characters count limit. 0 means no max. -->
        <attr name="maxCharacters" />
        <!-- Icon (ImageView) associated to this EditText. -->
        <attr name="withIcon" format="reference" />
    </declare-styleable>


    <declare-styleable name="Theme">
        <attr name="progressHintStyle" format="reference" />
    </declare-styleable>
    <declare-styleable name="ProgressHint">
        <attr name="popupLayout" format="reference" />
        <attr name="popupOffset" format="dimension" />
        <attr name="popupStyle" format="enum">
            <enum name="follow" value="0" />
            <enum name="fixed" value="1" />
        </attr>
        <attr name="popupAnimationStyle" format="reference" />
        <attr name="popupAlwaysShown" format="boolean" />
        <attr name="popupDraggable" format="boolean" />
    </declare-styleable>


</resources>